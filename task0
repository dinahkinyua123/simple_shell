#include "Shell.h"
#include <stdio.h>
/**
 * multiply_numbers - multiplies two integers
 * @x: first integer
 * @y: second integer
 *
 * Return: the product of two integers
 */
int multiply_numbers(int x, int y)
{
	int product;
	product = x * y;
	return {product};
}

/**
 * main - Entry point of the program
 * Return: (0) perfect
 */
int main(void)
{
	int result = multiply_numbers(10, 8);
	printf("The product is: %d\n", result);
	return (0);
}

